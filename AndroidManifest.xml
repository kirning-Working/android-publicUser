<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kirno.take_out_system"
    android:versionCode="1"
    android:versionName="1.0" >

    <!-- 使用的sdk -->
    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <!-- 应用信息 -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- 注册Activity -->
        <activity
            android:name="com.kirno.take_out_system.view.ActivityMain"
            android:label="@string/app_name" >
            <!-- 设置主启动Activiy -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kirno.take_out_system.view.ActivityCaiInf"
            android:label="@string/title_activity_caiinf" >
        </activity>
        <activity
            android:name="com.kirno.take_out_system.view.ActivityLogin"
            android:label="@string/title_activity_loginj" >
        </activity>
        <activity
            android:name="com.kirno.take_out_system.view.ActivityAllOrder"
            android:label="@string/title_activity_allOrder" >
        </activity>
    </application>

</manifest>